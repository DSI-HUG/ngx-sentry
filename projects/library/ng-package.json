{
    "$schema": "../../node_modules/ng-packagr/ng-package.schema.json",
    "dest": "../../dist/library",
    "assets": [
        "bin"
    ],
    "lib": {
        "entryFile": "src/public-api.ts",
        "umdModuleIds": {
            "@sentry/angular": "sentryAngular",
            "@sentry/tracing": "sentryTracing"
        }
    },
    "allowedNonPeerDependencies": [
        "@sentry/angular",
        "@sentry/tracing",
        "@sentry/cli",
        "ts-morph"
    ]
}
