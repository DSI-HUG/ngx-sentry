{
    "$schema": "http://json-schema.org/schema",
    "id": "sentry",
    "title": "sentry schematic",
    "type": "object",
    "properties": {
        "projectName": {
            "x-prompt": "What is your Sentry's project name ?",
            "description": "Sentry project name",
            "type": "string"
        },
        "sentryUrl": {
            "x-prompt": "What is your Sentry's dsn url ?",
            "description": "Sentry project dsn",
            "type": "string"
        }
    },
    "required": [
        "projectName",
        "sentryUrl"
    ],
    "additionalProperties": false
}
