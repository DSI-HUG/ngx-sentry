{
  "name": "ngx-sentry",
  "version": "0.0.1",
  "scripts": {
    "start": "../../node_modules/.bin/watch 'npm run build' schematics",
    "build": "../../node_modules/.bin/tsc -p tsconfig.schematics.json",
    "test": "npm run build && ../../node_modules/.bin/jasmine ../../dist/ngx-sentry/schematics/**/*.spec.js",
    "copy:collection": "../../node_modules/.bin/cpy --parents schematics/collection.json ../../dist/ngx-sentry",
    "copy:schemas": "../../node_modules/.bin/cpy --parents schematics/**/schema.json ../../dist/ngx-sentry",
    "postbuild": "npm run copy:collection && npm run copy:schemas"
  },
  "peerDependencies": {
    "@angular/common": ">=11.0.0",
    "@angular/core": ">=11.0.0"
  },
  "dependencies": {
    "@sentry/angular": "^6.0.0",
    "@sentry/tracing": "^6.0.0",
    "tslib": "^2.0.0"
  },
  "schematics": "./schematics/collection.json",
  "ng-add": {
    "save": "dependencies"
  },
  "devDependencies": {}
}
