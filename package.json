{
    "name": "@hug/ngx-sentry",
    "version": "1.2.0",
    "description": "Angular wrapper for Sentry JavaScript SDK",
    "homepage": "https://github.com/DSI-HUG/ngx-sentry",
    "license": "GPL-3.0-only",
    "author": "HUG",
    "contributors": [
        "rtrompier (https://github.com/rtrompier)",
        "badisi (https://github.com/badisi)"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/DSI-HUG/ngx-sentry.git"
    },
    "keywords": [
        "angular",
        "sentry",
        "sentry-client",
        "ngx-sentry",
        "monitor",
        "monitoring",
        "crash-reporting",
        "error-monitoring",
        "crash-reports",
        "csp-report",
        "error-logging",
        "error-handling",
        "http-interceptor",
        "http-errors"
    ],
    "bin": {
        "ngx-sentry": "./bin/index.js"
    },
    "scripts": {
        "start": "node ./build --watch",
        "build": "node ./build --build",
        "test:schematics": "cd projects/schematics; ts-node ../../node_modules/.bin/jasmine --config=jasmine.json; cd ../..;",
        "test:lib": "ng test library",
        "lint": "ng lint"
    },
    "dependencies": {
        "@badisi/latest-version": "^2.0.1",
        "@sentry/angular": "^6.0.0",
        "@sentry/cli": "^1.0.0",
        "@sentry/tracing": "^6.0.0",
        "ts-morph": "^10.0.0",
        "tslib": "^2.0.0"
    },
    "peerDependencies": {
        "@angular/common": "^11.0.0",
        "@angular/core": "^11.0.0"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^0.1102.12",
        "@angular/cli": "^11.2.12",
        "@angular/compiler-cli": "^11.2.13",
        "@angular/platform-browser-dynamic": "^11.2.13",
        "@hug/eslint-config": "^2.2.0",
        "@types/jasmine": "^3.7.1",
        "@types/node": "^15.0.2",
        "chokidar": "^3.5.1",
        "colors": "^1.4.0",
        "cpy": "^8.1.2",
        "eslint": "^7.26.0",
        "jasmine": "^3.7.0",
        "jasmine-core": "~3.7.1",
        "jasmine-spec-reporter": "~7.0.0",
        "karma": "~6.3.2",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "~2.0.3",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "^1.6.0",
        "ng-packagr": "^11.0.0",
        "protractor": "~7.0.0",
        "ts-node": "~9.1.1",
        "typescript": "~4.1.5"
    },
    "schematics": "./schematics/collection.json",
    "ng-add": {
        "save": "dependencies"
    }
