{
    "name": "@hug/ngx-sentry",
    "version": "1.2.0",
    "description": "Angular wrapper for Sentry JavaScript SDK",
    "homepage": "https://github.com/DSI-HUG/ngx-sentry",
    "license": "GPL-3.0-only",
    "author": "HUG - Hôpitaux Universitaires Genève",
    "contributors": [
        "rtrompier (https://github.com/rtrompier)",
        "badisi (https://github.com/badisi)"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/DSI-HUG/ngx-sentry.git"
    },
    "keywords": [
        "angular",
        "sentry",
        "sentry-client",
        "ngx-sentry",
        "monitor",
        "monitoring",
        "crash-reporting",
        "error-monitoring",
        "crash-reports",
        "csp-report",
        "error-logging",
        "error-handling",
        "http-interceptor",
        "http-errors"
    ],
    "bin": {
        "ngx-sentry": "./bin/index.js"
    },
    "scripts": {
        "start": "node ./build --watch",
        "build": "node ./build --build",
        "test:schematics": "cd projects/schematics; ts-node --project=tsconfig.spec.json ../../node_modules/.bin/jasmine --config=jasmine.json; cd ../..;",
        "test:lib": "ng test library",
        "lint": "ng lint"
    },
    "peerDependencies": {
        "@angular/core": ">= 11",
        "@angular/router": ">= 11"
    },
    "dependencies": {
        "@badisi/latest-version": "^2.0.3",
        "@sentry/angular": "^6.7.2",
        "@sentry/cli": "^1.66.0",
        "@sentry/tracing": "^6.7.2",
        "semver": "^7.3.5",
        "ts-morph": "^11.0.0",
        "tslib": "^2.3.0"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^12.0.5",
        "@angular/cli": "^12.0.5",
        "@angular/compiler-cli": "^12.0.5",
        "@angular/platform-browser-dynamic": "^12.0.5",
        "@hug/eslint-config": "^3.0.1",
        "@types/jasmine": "^3.7.7",
        "@types/node": "^15.12.4",
        "@types/semver": "^7.3.6",
        "chokidar": "^3.5.2",
        "colors": "^1.4.0",
        "cpy": "^8.1.2",
        "eslint": "^7.29.0",
        "jasmine": "^3.7.0",
        "jasmine-core": "~3.7.1",
        "jasmine-spec-reporter": "~7.0.0",
        "karma": "^6.3.4",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "~2.0.3",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "^1.6.0",
        "ng-packagr": "^12.0.8",
        "ts-node": "^10.0.0",
        "typescript": "~4.2.4"
    },
    "schematics": "./schematics/collection.json",
    "ng-add": {
        "save": "dependencies"
    }
}
